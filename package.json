{
  "name": "com.akylas.bpxcel2pdf",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akylas/pbexcel2pdf.git"
  },
  "author": {
    "name": "Martin Guillon",
    "email": "martin@akylas.fr"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Akylas/pbexcel2pdf/issues"
  },
  "homepage": "https://github.com/Akylas/pbexcel2pdf#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true
  },
  "private": true,
  "scripts": {
    "dev": "tauri dev",
    "dev:web": "vite",
    "tauri": "tauri",
    "build": "tauri build",
    "build:web": "vite build",
    "test": "cargo test --manifest-path ./src-tauri/Cargo.toml",
    "format": "prettier --write --plugin-search-dir=. src *.js && eslint --fix src *.js",
    "check": "cargo check --manifest-path src-tauri/Cargo.toml && eslint src *.js && svelte-check",
    "release": "standard-version --commit-all --preset angular"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@sveltejs/vite-plugin-svelte": "1.3.1",
    "@tauri-apps/api": "1.2.0",
    "@tauri-apps/cli": "1.2.0",
    "@tauri-apps/toml": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.56.1",
    "standard-version": "^9.5.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "carbon-components": "10.58.3",
    "carbon-components-svelte": "^0.70.12",
    "carbon-icons-svelte": "^11.4.0",
    "convert-excel-to-json": "^1.7.0",
    "events": "^3.3.0",
    "svelte-i18n": "^3.5.2",
    "svelte-tauri-filedrop": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "version": "1.1.0"
}
